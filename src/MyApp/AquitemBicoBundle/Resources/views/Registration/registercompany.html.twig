{% extends "MyAppAquitemBicoBundle::layout.html.twig" %}


{% block head %}
    <title> {% block title %} {{parent() }}{% endblock %} </title>
    {% block stylesheet %} 
        {{parent() }}
        <link rel="stylesheet" href="{{ asset('assets/css/multistepsform.css') }}">
    {% endblock %}

{% endblock %}

{% block content %}
    <div class="page_banner login_banner">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <div class="banner-heading">Cadastro</div>    
                </div>  
            </div>
        </div>
    </div> 
    <div id="maincontent">    
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="page-tab">
                    <div id="form">
                        <div id="userform">
                            <ul class="nav nav-tabs nav-justified" role="tablist">
                                <li class="active border-right"><a href="#individualEmp"  role="tab" data-toggle="tab">Eu sua pessoa física</a></li>
                                <li><a href="#legalCompany" role="tab" data-toggle="tab">Eu sua pessoa júridica</a></li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane fade active in" id="individualEmp">
                                    <form id="individualEmp">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Data de nascimento</label><label style="color: red">*</label>
                                                <input type="date" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                <!--<div class="search_icon"><span class="ti-calendar"></span></div>-->
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Sexo</label><label style="color: red">*</label>
                                                <select  name="time" class="form-control">
                                                    <option value="F">Feminino</option>
                                                    <option value="M">Masculino</option>
                                                </select>
                                                <!--<div class="search_icon"><span class="ti-email"></span></div>-->
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Endereço</label><label style="color: red">*</label>
                                                <input type="text" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                <div class="search_icon"><span class="ti-pin"></span></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Telefone</label><label style="color: red">*</label>
                                                <input type="number" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                <!--<div class="search_icon"><span class="ti-c"></span></div>-->
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Profissão</label><label style="color: red">*</label>
                                                <input type="text" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                <!--<div class="search_icon"><span class="ti-user"></span></div>-->
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>CPF</label><label style="color: red">*</label>
                                                <input type="email" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                <!--<div class="search_icon"><span class="ti-user"></span></div>-->
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="checkbox">
                                                <label><input type="checkbox" value="">Desejo receber informações do <span style="color: #d36c00;">Aqui Tem Bico    </span> por e-mail.</label>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="checkbox">
                                                <label><input type="checkbox" value="">Concordo com os <a href="{{ path('aquitem_bico_termsandconditions') }}" style="color: #d36c00;">termos de uso</a> do Aqui Tem Bico.</label>
                                            </div>
                                        </div>
                                        <div class="mrgn-30-top">
                                            <button type="submit" class="btn btn-larger btn-block"/>Cadastre-se</button>
                                        </div>
                                    </form>
                                </div>
                                <div class="tab-pane fade" id="legalCompany">
                                    <div class="row">
                                        <form id="legalCompany" action="{{ path('aquitem_bico_registercompany') }}" >
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>CNPJ</label><label style="color: red">*</label>
                                                    <input type="number" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                    <!--<div class="search_icon"><span class="ti-c"></span></div>-->
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Telefone</label><label style="color: red">*</label>
                                                    <input type="email" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                    <!--<div class="search_icon"><span class="ti-c"></span></div>-->
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Endereço</label><label style="color: red">*</label>
                                                    <input type="text" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                    <div class="search_icon"><span class="ti-pin"></span></div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Area de actuação</label><label style="color: red">*</label>
                                                    <input type="text" class="form-control" id="email" required data-validation-required-message="Por favor, insira seu email." autocomplete="off">
                                                    <div class="search_icon"><span class="ti-pin"></span></div>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="checkbox">
                                                    <label><input type="checkbox" value="">Desejo receber informações do <span style="color: #d36c00;">Aqui Tem Bico    </span> por e-mail.</label>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="checkbox">
                                                    <label><input type="checkbox" value="">Concordo com os <a href="{{ path('aquitem_bico_termsandconditions') }}" style="color: #d36c00;">termos de uso</a> do Aqui Tem Bico.</label>
                                                </div>
                                            </div>
                                            <div class="mrgn-30-top">
                                                <button type="submit" class="btn btn-larger btn-block"/>Cadastre-se</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div> 
        </div>
    </div>
{% endblock %}


{% block js %} 
    {{parent() }}
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js'></script>
    <script src="{{ asset('assets/js/multistepsform.js') }}"></script>
{% endblock %}

