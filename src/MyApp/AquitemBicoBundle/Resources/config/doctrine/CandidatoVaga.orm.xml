<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MyApp\AquitemBicoBundle\Entity\CandidatoVaga" table="candidato_vaga">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCandidato" type="integer" column="ID_candidato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idVaga" type="integer" column="ID_vaga" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
