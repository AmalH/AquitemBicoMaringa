<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MyApp\AquitemBicoBundle\Entity\HistoricoEmpresa" table="historico_empresa">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEmpresa" type="integer" column="ID_empresa" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idCandidato" type="integer" column="ID_candidato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="avaliacao" type="float" column="avaliacao" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
